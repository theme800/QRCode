<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net452</TargetFrameworks>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageId>Theme800.QRCode</PackageId>
    <Version>1.0.1</Version>
    <Title>二维码显示插件</Title>
    <PackageVersion>1.0.1</PackageVersion>
    <Authors>Theme800</Authors>
    <Company></Company>
    <Product></Product>
    <Description>二维码插件用于在模板中通过标签将 URL 地址生成二维码图片并展示在页面中。</Description>
    <PackageLicense>MIT</PackageLicense>
    <PackageProjectUrl>https://github.com/theme800/Theme800.QRCode</PackageProjectUrl>
    <PackageIconUrl>https://www.siteserver.cn/docs/theme800/qrcode/logo.svg</PackageIconUrl>
    <Copyright></Copyright>
    <PackageTags>SiteServer CMS 二维码</PackageTags>
    <RepositoryUrl>https://github.com/theme800/Theme800.QRCode</RepositoryUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes></PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SiteServer.Plugin" Version="2.3.11" />
  </ItemGroup>
  
  <ItemGroup>
    <Content Include="qrcode.min.js" />
  </ItemGroup>

</Project>